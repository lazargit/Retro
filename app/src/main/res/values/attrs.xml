<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Options">
        <attr name="titleText" format="string" localization="suggested" />
        <attr name="valueColor" format="color" />
    </declare-styleable>


    <declare-styleable name="ImageSliderView">
        <attr name="showImageFX" format="boolean" />
        <attr name="showCircleIndicator" format="boolean" />
        <attr name="showTitle" format="boolean" />
        <attr name="pagefx" format="enum">
            <enum name="Slide" value="0"/>
            <enum name="FadeOut" value="1"/>
            <enum name="ZoomOut" value="2"/>
            <enum name="Pop" value="3"/>
        </attr>

    </declare-styleable>


    <declare-styleable name="RetroImageView">
        <attr name="showFX" format="boolean" />


        <attr name="fx_duration" format="float" />

        <attr name="fx_interpolator" format="float" />

        <attr name="labelPosition" format="enum">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
        </attr>
    </declare-styleable>


</resources>